{
  "AppAuth": {
    "ApiKey": "dev-secret-key-change-in-production"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PicturesData": {
    "skipSuffix": ["_skip", "-skip", "_pss", "-pss", "_noW", "-noW"],
    "skipPrefix": ["skip_", "skip-", "pss_", "pss-", "noW_", "noW-"],
    "skipContains": ["EosRP", "_thumbnails"],
    "roleSuffix":["_private", "_public", "-family", " extfamily", "_friends", "_custid"],
    "rolePrefix":["private_", "public_", "family_", "extfamily_", "friends_", "custid_"],
    "valueBasedRolePrefix":["custid_{X}_", "custid-{X}-", "custid {X} "],
    "valueBasedRoleSuffix":["_custid_{X}", "-custid-{X}", " custid {X}"],
    "featurePhotoSuffix": ["_label", "_feature"],
    "featurePhotoPrefix": ["label_", "feature_"],
    "imageExtensions": [ ".jpg", ".jpeg", ".png", ".webp" ],
    "movieExtensions": [ ".mp4", ".mov", ".avi", ".3gp" ]
  },
  "Smtp": {
    "AuthType": "Basic",
    "Host": "smtp.gmail.com",
    "Port": 587,
    "User": "your-user@gmail.com",
    "Pass": "your-app-password-here",  
    "From": "your-user@gmail.com",
    "OAuth": {
      "ClientId": "your-client-id.apps.googleusercontent.com",
      "ClientSecret": "your-client-secret",
      "RefreshToken": "your-refresh-token"
    }
  },
  "SendGrid": {
    "ApiKey": "your-sendgrid-api-key",
    "From": "noreply@ecogallery.net"
  }
}
