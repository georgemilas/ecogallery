{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Database": {
    "Host": "localhost",
    "Port": 5432,
    "Database": "ecogallery",
    "Username": "postgres",
    "SslMode": "Disable"
  },
  "PicturesData": {
    "skipSuffix": ["_skip", "-skip", "_pss", "-pss", "_noW", "-noW"],
    "skipPrefix": ["skip_", "skip-", "pss_", "pss-", "noW_", "noW-"],
    "skipContains": ["EosRP", "_thumbnails"],
    "roleSuffix":["_private", "_public", "-family", " extfamily", "_friends", "_custid"],    
    "rolePrefix":["private_", "public_", "family_", "extfamily_", "friends_", "custid_"],    
    "valueBasedRolePrefix":["custid_{X}_", "custid-{X}-", "custid {X} "],
    "valueBasedRoleSuffix":["_custid_{X}", "-custid-{X}", " custid {X}"],    
    "featurePhotoSuffixOrPrefix": ["label", "feature"],
    "imageExtensions": [ ".jpg", ".jpeg", ".png", ".webp" ],
    "movieExtensions": [ ".mp4", ".mov", ".avi", ".3gp" ]
  }
}
